  "shortcuts": [
    {
      "name": "Dashboard",
      "short_name": "Dashboard",
      "description": "View system dashboard",
      "url": "/dashboard",
      "icons": [
        {
          "src": "/icons/shortcut-dashboard.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Tenants",
      "short_name": "Tenants",
      "description": "Manage tenants",
      "url": "/tenants",
      "icons": [
        {
          "src": "/icons/shortcut-tenants.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Users",
      "short_name": "Users",
