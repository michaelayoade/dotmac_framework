        {
          "src": "/icons/shortcut-users.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "share_target": {
    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "file",
          "accept": ["application/json", "text/csv", ".xlsx"]
        }
      ]
    }
  },
  "protocol_handlers": [
    {
      "protocol": "web+dotmac",
      "url": "/protocol-handler?url=%s"
    }
  ],
  "prefer_related_applications": false,
  "related_applications": [],
  "scope": "/",
  "id": "dotmac-management-admin",
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "handle_links": "preferred"
}