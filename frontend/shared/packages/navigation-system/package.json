{
  "name": "@dotmac/navigation-system",
  "version": "0.1.0",
  "description": "Universal navigation system for DotMac ISP Framework",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "navigation",
    "react",
    "typescript",
    "sidebar",
    "breadcrumbs",
    "mobile",
    "tabs",
    "dotmac"
  ],
  "author": "DotMac Framework Team",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.263.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
