{
  "title": "Management Platform Overview",
  "tags": ["dotmac", "management", "overview"],
  "panels": [
    {
      "title": "HTTP Requests (rate)",
      "type": "timeseries",
      "queries": [
        {
          "metricName": "http_requests_total",
          "operation": "RATE",
          "legend": "{method} {endpoint}",
          "filters": {"service": "dotmac-management"},
          "groupBy": ["method", "endpoint"]
        }
      ]
    },
    {
      "title": "HTTP Request Duration P95",
      "type": "timeseries",
      "queries": [
        {
          "metricName": "http_request_duration_seconds",
          "operation": "P95",
          "legend": "p95",
          "filters": {"service": "dotmac-management"}
        }
      ]
    },
    {
      "title": "API Success Rate",
      "type": "timeseries",
      "queries": [
        {"metricName": "api_request_success_rate_success", "operation": "RATE", "as": "s", "filters": {"service": "dotmac-management"}},
        {"metricName": "api_request_success_rate_total", "operation": "RATE", "as": "t", "filters": {"service": "dotmac-management"}}
      ],
      "formula": "s / t",
      "legend": "API Success Rate"
    },
    {
      "title": "Business: Partner Signups",
      "type": "timeseries",
      "queries": [
        {"metricName": "dotmac_partner_signups_total", "operation": "RATE", "legend": "signups"}
      ]
    }
  ]
}

